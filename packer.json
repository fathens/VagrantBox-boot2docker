{
  "variables": {
    "vagrant_token": "{{env `VAGRANT_TOKEN`}}"
  },
  "builders": [
    {
      "type": "virtualbox-iso",
      "iso_url": "https://github.com/boot2docker/boot2docker/releases/download/v1.11.1/boot2docker.iso",
      "iso_checksum": "9be71967c5b43eb6468d45e4e3e0815d",
      "iso_checksum_type": "md5",
      "ssh_username": "docker",
      "ssh_password": "tcuser",
      "shutdown_command": "echo 'packer' | sudo -S shutdown -h now",
      "headless": true,
      "guest_additions_mode": "disable",
      "guest_os_type": "Linux26_64"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "script": "provision.sh"
    }
  ],
  "post-processors": [
    [
      {
        "type": "vagrant"
      },
      {
        "type": "vagrant-cloud",
        "access_token": "{{user `vagrant_token`}}",
        "box_tag": "fathens/boot2docker",
        "version": "0.0.1"
      }
    ]
  ]
}
